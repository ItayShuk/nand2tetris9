class Points{

    field int x;
    field int y;

    constructor Points new(int p_x, int p_y){
        let x = p_x;
        let y = p_y;
        return this;
    }

    method int getX(){
        return x;
    }

    method int getY(){
        return y;
    }

    method void setX(int num){
        let x = num;
        return;
    }

    method void setY(int num){
        let y = num;
        return;
    }

    method void dispose(){
    do Memory.deAlloc(this);
    return;
    }
}